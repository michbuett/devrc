[color]
    branch = auto
    status = auto
    diff = auto
    ui = auto
    log = auto
[color "diff"]
    meta = yellow
    frag = cyan
    old = red
    new = green
    whitespace = red reverse
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[color "interactive"]
     promt = yellow
     header =cyan
     help = green
     error = red
[alias]
    st = status
    rb = !git stash && git svn rebase && git stash pop
    dc = !git stash && git svn dcommit && git stash pop
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    root = rev-parse --show-toplevel
[core]
    pager = less -XRMSI
